      <h2>
                </h2>
      <div class="news clearfix" style="color:white">
                      <div id="datetimepicker4" class="input-append">
                <input data-format="dd.MM.yyyy" type="text"></input>
                <span class="add-on">
                  <i data-time-icon="icon-time" data-date-icon="icon-calendar">
                  </i>
                </span>
              </div>

            <script type="text/javascript">
              $(function() {
                $('#datetimepicker4').datetimepicker({
                  pickTime: false,
                  endDate: this.localDate  
                });
              });
              $('#datetimepicker4').on('changeDate', function(e) {

                function getAge(fromdate){
                  
									
									
									var oneDay = 24*60*60*1000; // hours*minutes*seconds*milliseconds
									var firstDate = new Date(fromdate);
									var secondDate = new Date();

									var diffDays = Math.round(Math.abs((firstDate.getTime() - secondDate.getTime())/(oneDay)));
									return diffDays+' dienas';


									
									fromdate.setDate(fromdate.getDate());
                  todate= new Date();
                  if (fromdate>=todate) return '0 dienas';

                  var age= [], fromdate= fromdate,
                  y= [todate.getFullYear(), fromdate.getFullYear()],
                  ydiff= y[0]-y[1],
                  m= [todate.getMonth(), fromdate.getMonth()],
                  mdiff= m[0]-m[1],
                  d= [todate.getDate(), fromdate.getDate()],
                  ddiff= d[0]-d[1];

                  if(mdiff < 0 || (mdiff=== 0 && ddiff<0))--ydiff;
                  if(mdiff<0) mdiff+= 11;
                  if(ddiff<0){
                      fromdate.setMonth(m[1]+1, 0);
                      ddiff= fromdate.getDate()-d[1]+d[0];
                      --mdiff;
                  }
                  if(ydiff> 0) age.push(ydiff+ ' gad'+(ydiff> 1? 'i ':'s'));
                  if(mdiff> 0) age.push(mdiff+ ' mēne'+(mdiff> 1? 'ši':'sis'));
                  if(ddiff> 0) age.push(ddiff+ ' diena'+(ddiff> 1? 's':''));
                  if(age.length>1) age.splice(age.length-1,0,' un ');    
                  return age.join('');
                } 
                $('#calc_dienas').text(getAge(e.date));
              });
            </script>
            <br>
            <h3><span id="calc_dienas">&nbsp;</span></h3>

